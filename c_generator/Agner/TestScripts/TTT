   718                              <1>  sub eax, [r13+i*4+4+(CountOverhead-ThreadData)]
   718                              <1> %endif
   718 000003E3 438944B53C          <1>  mov [r13+r14*4+i*4*REPETITIONS+(PMCResults-ThreadData)], eax
