   686                                  ;        sub     [r13 + (CountTemp-ThreadData)], eax        ; CountTemp[0]
   687                                  
   688                                  ;        SERIALIZE
   689                                  
   690                                          ; Read counters
   691                                  %assign i  0
   692                                  %rep    NUM_COUNTERS
   693                                          mov     ecx, [Counters + i*4]
   694                                          rdpmc
   695                                          sub     [r13 + i*4 + 4 + (CountTemp-ThreadData)], eax  ; CountTemp[i+1]
   696                                  %assign i  i+1
   697                                  %endrep
