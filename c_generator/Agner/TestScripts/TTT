   697 000003D3 0F33                <1>  rdpmc
   697 000003D5 41294504            <1>  sub [r13 + i*4 + 4 + (CountTemp-ThreadData)], eax
   697                              <1> %assign i i+1
   698                                  
   699                                          SERIALIZE
   699 000003D9 31C0                <1>  xor eax, eax
   699 000003DB 0FA2                <1>  cpuid
   700                                  
   701                                          ; subtract counts before from counts after
   702                                  ;        mov     eax, [r13 + (CountTemp-ThreadData)]            ; -count
   703                                   ;       neg     eax
   704                                  %if     SUBTRACT_OVERHEAD
   705                                          sub     eax, [r13+(CountOverhead-ThreadData)]   ; overhead clock count        
   706                                  %endif  ; SUBTRACT_OVERHEAD        
   707                                    ;      mov     [r13+r14*4+(ClockResults-ThreadData)], eax      ; save clock count
   708                                          
   709                                  %assign i  0
   710                                  %rep    NUM_COUNTERS
   711                                          mov     eax, [r13 + i*4 + 4 + (CountTemp-ThreadData)]
   712                                          neg     eax
   713                                  %if     SUBTRACT_OVERHEAD
   714                                          sub     eax, [r13+i*4+4+(CountOverhead-ThreadData)]   ; overhead pmc count        
   715                                  %endif  ; SUBTRACT_OVERHEAD        
   716                                          mov     [r13+r14*4+i*4*REPETITIONS+(PMCResults-ThreadData)], eax      ; save count        
   717                                  %assign i  i+1
   718                                  %endrep
